{
  "name": "quinen/php-lib",
  "description": "quinen's personnal library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Laurent Desmonceaux",
      "email": "laurent@quinen.net"
    }
  ],
  "require": {
    "bramus/ansi-php": "^3.0",
    "cakephp/collection": "3.*",
    "cakephp/orm": "3.*",
    "cakephp/utility": "3.*",
    "psr/container": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "QuinenLib\\": "./src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  }
}
